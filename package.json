{
  "name": "mostreadable",
  "version": "0.1.0",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "mostReadable",
    "manifest": "src/manifest.json",
    "main": "mostreadable.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.5.11",
    "@ant-design/colors": "^2.0.4",
    "tinycolor2": "^1.4.1"
  },
  "author": "whaleExplorer <whaleExplorer@gmail.com>"
}
